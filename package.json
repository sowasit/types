{
  "name": "@sowasit/types",
  "version": "0.1.0",
  "description": "SoWasIt type definitions and contracts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
