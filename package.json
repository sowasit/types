  "keywords": [
    "api",
    "types",
    "contracts"
  ],
  "author": "Askelia",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.19.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
